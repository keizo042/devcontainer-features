{
    "name": "host home share",
    "id": "host-home-share",
    "version": "1.0.0",
    "description": "container access host user home directory",
    "options": {
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ],
    "mounts": [
	    "source=${localEnv:HOME}${localEnv:USERPROFILE},target=/var/tmp/home,type=bind,consistency=cached"
    ]
}

